package Conditions_and_loops;

import java.util.Scanner;

public class SumOfSeries {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan=new Scanner(System.in);
		int n=scan.nextInt();
		double sum=0;
		
		for(int i=1;i<=n;i++)
		{
			sum += sum + 1/i;
		}
		System.out.println(sum);

	}

}

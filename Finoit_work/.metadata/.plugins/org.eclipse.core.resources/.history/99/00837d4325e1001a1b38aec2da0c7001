package Conditions_and_loops;

import java.util.Scanner;

public class Power {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan=new Scanner(System.in);
		int x=scan.nextInt();
		int n=scan.nextInt();
		int pow=1;
		//now we find the value of x have a power n
		for(int i=0;i<n;i++)
		{
			pow=pow*x;
			
		}
	

	}

}
